class bank:
    def __init__(self):
        self.accno=int(input("Enter the account number: "))
        self.acctype=(input("Enter the account type: "))
        self.holdname=(input("Enter the account holder name: "))
        self.balance=int(input("Enter the account balance: "))
    def deposit(self):
        amount=int(input("Enter the amount to deposit:"))
        self.balance+=amount
        print("The account balance is:",self.balance)
    def withdraw(self):
        amount=int(input("Enter the amount to withdraw:"))
        if self.balance>=amount:
            self.balance-=amount
            print("YOUR WITHDRAWED AMOUNT IS:",amount)
        else:
            print("INSUFFICIENT BALANCE TO WITHDRAW")
    def display(self):
        print("Net available balance is:",self.balance)


a=bank()
a.deposit()
a.withdraw()
a.display()
